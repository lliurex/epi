#!/bin/sh

set -e

case "$1" in
	remove|purge|abort-install|disappear)
		dpkg-divert --package epi-local-installer --rename --quiet --remove /usr/share/applications/kde-mimeapps.list.real || true
;;
	upgrade|failed-upgrade|abort-upgrade)
	;;
	*)
		echo "prerm called with unknown argument $1" >&2
		exit 1
	;;
esac
#DEBHELPER#

exit 0
