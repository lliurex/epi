#: epi-gtk/python3-epigtk/MainWindow.py:332
msgid "Accept Eula and Install"
msgstr "Accepta la llicència i instal·la"

#: epi-gtk/python3-epigtk/MainWindow.py:334
#: epi-gtk/python3-epigtk/rsrc/epi-gtk.ui:576
msgid "Install"
msgstr "Instal·la"

#: epi-gtk/python3-epigtk/MainWindow.py:339
msgid "Reinstall"
msgstr "Reinstal·la"

#: epi-gtk/python3-epigtk/MainWindow.py:975
msgid "Application already installed"
msgstr "L'aplicació ja està instal·lada."

#: epi-gtk/python3-epigtk/MainWindow.py:977
msgid "Application epi file does not exist"
msgstr "El fitxer .epi de l'aplicació no existeix."

#: epi-gtk/python3-epigtk/MainWindow.py:979
msgid "You need root privileges"
msgstr "Necessiteu permisos d'administració."

#: epi-gtk/python3-epigtk/MainWindow.py:981
msgid "Internet connection not detected"
msgstr "No s'ha detectat connexió a Internet."

#: epi-gtk/python3-epigtk/MainWindow.py:983
msgid "Gathering Information..."
msgstr "S'està obtenint informació..."

#: epi-gtk/python3-epigtk/MainWindow.py:985
msgid "Downloading application..."
msgstr "S'està descarregant l'aplicació..."

#: epi-gtk/python3-epigtk/MainWindow.py:987
msgid "Preparing installation..."
msgstr "S'està preparant la instal·lació..."

#: epi-gtk/python3-epigtk/MainWindow.py:989
msgid "Installing application..."
msgstr "S'està instal·lant l'aplicació..."

#: epi-gtk/python3-epigtk/MainWindow.py:991
msgid "Ending the installation..."
msgstr "Està finalitzant la instal·lació..."

#: epi-gtk/python3-epigtk/MainWindow.py:993
msgid "Installation completed successfully"
msgstr "La instal·lació s'ha completat correctament."

#: epi-gtk/python3-epigtk/MainWindow.py:995
msgid "Installation aborted. Error ending installation"
msgstr ""
"La instal·lació s'ha cancel·lat. S'ha produït un error en finalitzar la "
"instal·lació."

#: epi-gtk/python3-epigtk/MainWindow.py:997
msgid "Installation aborted. Error installing application"
msgstr ""
"La instal·lació s'ha cancel·lat. S'ha produït un error en instal·lar "
"l'aplicació."

#: epi-gtk/python3-epigtk/MainWindow.py:999
msgid "Installation aborted. Error preparing system"
msgstr ""
"La instal·lació s'ha cancel·lat. S'ha produït un error en preparar el "
"sistema."

#: epi-gtk/python3-epigtk/MainWindow.py:1001
msgid "Installation aborted. Unable to download package"
msgstr "La instal·lació s'ha cancel·lat. No s'ha pogut descarregar el paquet."

#: epi-gtk/python3-epigtk/MainWindow.py:1003
msgid "Do you want uninstall the application?"
msgstr "Voleu desinstal·lar l'aplicació?"

#: epi-gtk/python3-epigtk/MainWindow.py:1005
msgid "Uninstall application..."
msgstr "S'està desinstal·lant l'aplicació..."

#: epi-gtk/python3-epigtk/MainWindow.py:1007
msgid "Application successfully uninstalled"
msgstr "L'aplicació s'ha desinstal·lat correctament."

#: epi-gtk/python3-epigtk/MainWindow.py:1009
msgid "Uninstalled process ending with errors"
msgstr "El procés de desinstal·lació ha finalitzat amb errors."

#: epi-gtk/python3-epigtk/MainWindow.py:1011
msgid "Application epi file it is not a valid json"
msgstr "El fitxer .epi de l'aplicació no és un json vàlid"

#: epi-gtk/python3-epigtk/EpiBox.py:159
msgid "Press to view application information"
msgstr "Premeu per a veure la informació de l'aplicació"

#: epi-gtk/python3-epigtk/EpiBox.py:161
msgid "Info not availabled"
msgstr "La informació no està disponible."

#: epi-gtk/python3-epigtk/ChooserBox.py:75
msgid "Please choose a epi file"
msgstr "Trieu un fitxer epi"

#: epi-gtk/python3-epigtk/rsrc/epi-gtk.ui:21
msgid "Choose epi file to install/uninstall the application:"
msgstr "Trieu un fitxer epi per a instal·lar/desinstal·lar l'aplicació:"

#: epi-gtk/python3-epigtk/rsrc/epi-gtk.ui:72
msgid "..."
msgstr "..."

#: epi-gtk/python3-epigtk/rsrc/epi-gtk.ui:105
msgid "Application to install"
msgstr "Aplicació per instal·lar"

#: epi-gtk/python3-epigtk/rsrc/epi-gtk.ui:158
msgid "(D) Addicitional application required"
msgstr "(D) Aplicació addicional requerida"

#: epi-gtk/python3-epigtk/rsrc/epi-gtk.ui:245
msgid "Eula for installing "
msgstr "Acord de llicència d'usuari final per a instal·lar "

#: epi-gtk/python3-epigtk/rsrc/epi-gtk.ui:468
msgid "Checking system..."
msgstr "S'està comprovant el sistema..."

#: epi-gtk/python3-epigtk/rsrc/epi-gtk.ui:611
msgid "Uninstall"
msgstr "Desinstal·la"
